{
  "name": "astroship",
  "type": "module",
  "version": "3.0.0",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "test:screenshots": "playwright test tests/test-runner.spec.ts --grep \"Screenshot Testing\"",
    "test:screenshots-desktop": "playwright test tests/test-runner.spec.ts --grep \"Screenshot Testing.*Desktop\"",
    "test:screenshots-laptop": "playwright test tests/test-runner.spec.ts --grep \"Screenshot Testing.*Laptop\"",
    "test:screenshots-mobile": "playwright test tests/test-runner.spec.ts --grep \"Screenshot Testing.*Mobile\"",
    "test:accessibility": "playwright test tests/test-runner.spec.ts --grep \"Accessibility Testing\"",
    "test:seo": "playwright test tests/test-runner.spec.ts --grep \"SEO Testing\"",
    "test:session": "playwright test tests/test-runner.spec.ts",
    "test:all": "playwright test tests/test-runner.spec.ts --grep \"Screenshot Testing|Accessibility Testing|SEO Testing\"",
    "test:clean": "rimraf test-results"
  },
  "dependencies": {
    "@astrojs/mdx": "^4.2.0",
    "@astrojs/sitemap": "^3.2.1",
    "@fontsource-variable/bricolage-grotesque": "^5.2.5",
    "@fontsource-variable/inter": "^5.2.5",
    "@iconify-json/bx": "^1.2.2",
    "@iconify-json/simple-icons": "^1.2.28",
    "@iconify-json/uil": "^1.2.3",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.0.14",
    "astro": "^5.5.2",
    "astro-icon": "^1.1.5",
    "astro-navbar": "^2.3.9",
    "astro-seo": "^0.8.4",
    "sharp": "^0.33.5",
    "tailwindcss": "^4.0.14"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@playwright/test": "^1.54.1",
    "playwright": "^1.54.1",
    "rimraf": "^6.0.1"
  }
}
